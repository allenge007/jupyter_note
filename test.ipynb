    "import sympy as sym\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from sympy.abc import x, theta, r, z, t\n",
    "\n",
    "# 定义 sigma 为正实数\n",
    "sigma = sym.Symbol('sigma', positive=True, real=True)\n",
    "\n",
    "pi = sym.pi\n",
    "\n",
    "# 定义函数 F\n",
    "F = sym.exp(-t/(2*sigma**2))\n",
    "\n",
    "sym.integrate(F, t)"
